<tr>
  <td><%= member_icon member %></td>
  <td><%= member_avatar_link(member) %></td>
  <td><%= member.first_name %></td>
  <td><%= member.last_name %></td>
  <td><%= member.email %></td>
  <td><%= member.time_zone.truncate(15) %></td>
  <td>
    <% if can? :update, member %>
      <%= link_to('Edit', edit_community_admin_member_path(current_community, member), class: 'btn btn-default btn-xs') %>
    <% end %>
  </td>
</tr>
